<workflow xmlns="http://vmware.com/vco/workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://vmware.com/vco/workflow http://vmware.com/vco/workflow/Workflow-v4.xsd" root-name="item1" object-name="workflow:name=generic" id="4ea77f57-7833-4b39-97a1-68757168d892" version="0.1.0" api-version="6.0.0" editor-version="2.0" restartMode="1" resumeFromFailedMode="0">
  <display-name><![CDATA[Get Virtual Machine DiskObjectId]]></display-name>
  <position y="50.0" x="100.0"/>
  <input>
    <param name="virtualMachine" type="string"/>
  </input>
  <output>
    <param name="UuidArray" type="Array"/>
    <param name="vm" type="VC:VirtualMachine"/>
  </output>
  <attrib name="virtualMachines" type="Array/VC:VirtualMachine" read-only="false">
    <value encoded="n"><![CDATA[[]]]></value>
  </attrib>
  <workflow-item name="item0" type="end" end-mode="0">
    <in-binding/>
    <position y="50.0" x="580.0"/>
  </workflow-item>
  <workflow-item name="item1" out-name="item2" type="link" linked-workflow-id="BB808080808080808080808080808080A180808001322751030482b80adf61e7c">
    <display-name><![CDATA[Get virtual machines by name]]></display-name>
    <in-binding>
      <bind name="criteria" type="string" export-name="virtualMachine">
        <description><![CDATA[Search criteria. Regular expressions supported.]]></description>
      </bind>
    </in-binding>
    <out-binding>
      <bind name="vms" type="Array/VC:VirtualMachine" export-name="virtualMachines">
        <description><![CDATA[Virtual machines found]]></description>
      </bind>
    </out-binding>
    <description><![CDATA[ ]]></description>
    <position y="60.0" x="200.0"/>
  </workflow-item>
  <workflow-item name="item2" out-name="item0" type="task">
    <display-name><![CDATA[Get DiskObjectId]]></display-name>
    <script encoded="false"><![CDATA[var devices = virtualMachines[0].config.hardware.device;
var vm = virtualMachines[0];

var UuidArray = new Array()

for each (device in devices) {
    if (device.DiskObjectId) {
        UuidArray.push(device);
    }
}]]></script>
    <in-binding>
      <bind name="virtualMachines" type="Array/VC:VirtualMachine" export-name="virtualMachines"/>
    </in-binding>
    <out-binding>
      <bind name="UuidArray" type="Array" export-name="UuidArray"/>
      <bind name="vm" type="VC:VirtualMachine" export-name="vm"/>
    </out-binding>
    <description><![CDATA[Simple task with custom script capability.]]></description>
    <position y="60.0" x="360.0"/>
  </workflow-item>
  <presentation/>
</workflow>